<div class="control-group">
  <label class="control-label">Registrar</label>
  <div class="controls">
    <input type="text" name="Registrar" placeholder="Registrar"
      value="<%= domains.registrar || '' %>"
      required
    >
  </div>
</div>
<div class="control-group">
  <label class="control-label">Action</label>
  <div class="controls">
    <input type="text" name="Action" placeholder="Action"
      value="<%= domains.action || '' %>"
      required
    >
  </div>
</div>
<div class="control-group">
  <label class="control-label">Renewed</label>
  <div class="controls">
    <input type="text" name="username" placeholder="domains"
      value="<%= domains.domainsname || '' %>"
      autocomplete="off"
      pattern="\S{4,}"
      title="Minimum of 4 characters, no white space"
    >
  </div>
</div>
<div class="control-group">
  <label class="control-label">Password</label>
  <div class="controls">
